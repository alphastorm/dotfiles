[user]
	name = Sunil Srivatsa
[branch]
	autosetuprebase = always
[rerere]
	enabled = true
[alias]
	r = !GIT_NO_PAGER=1 git l -30
	l = "!source ~/.githelpers && pretty_git_log"
[push]
	default = current
	followTags = true
[pager]
	diff = diff-so-fancy | less --tabs=4 -RFX
	show = diff-so-fancy | less --tabs=4 -RFX
