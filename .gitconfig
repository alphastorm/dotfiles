[user]
  name = Sunil Srivatsa
[branch]
  autosetuprebase = always
[rerere]
  enabled = true
[alias]
  l = "!source ~/.githelpers && pretty_git_log"
  r = !GIT_NO_PAGER=1 git l -30
  patch = !git --no-pager diff --no-color
[core]
  pager = diff-so-fancy | less --tabs=4 -RFX
[push]
  default = current
  followTags = true
